    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Windows) Launch",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/bin/packer.exe",
            "args": [
                "-f",
                "-i",
                "${workspaceFolder}/examples/data/example_data.json",
                "-o",
